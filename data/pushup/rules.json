{
  "keypoints": ["shoulder", "elbow", "wrist", "hip", "knee", "ankle"],
  "angle_rules": [
    {
      "joints": ["shoulder", "elbow", "wrist"],
      "min_angle_lower": 5,
      "min_angle_upper": 60,
      "max_angle_lower": 170,
      "max_angle_upper": 180,
      "feedback": "Elbow angle too large. Go further down.",
      "feedback_motion": "",
      "sides": {
        "left": ["lshoulder", "lelbow", "lwrist"],
        "right": ["rshoulder", "relbow", "rwrist"]
      }
    },
    {
      "joints": ["hip", "knee", "ankle"],
      "min_angle_lower": 130,
      "min_angle_upper": 180,
      "feedback": "Keep your knees straight during the exercise.",
      "feedback_motion": "",
      "sides": {
        "left": ["lhip", "lknee", "lankle"],
        "right": ["rhip", "rknee", "rankle"]
      }
    },
    {
      "joints": ["shoulder", "hip", "ankle"],
      "min_angle_lower": 130,
      "min_angle_upper": 180,
      "feedback": "Keep your back straight during the exercise.",
      "feedback_motion": "",
      "sides": {
        "left": ["lshoulder", "lhip", "lankle"],
        "right": ["rshoulder", "rhip", "rankle"]
      }
    }
  ]
}
